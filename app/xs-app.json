{
  "welcomeFile": "app/launchpad.html",
  "routes": [
    {
      "source": "^/app/(.*)$",
      "target": "$1",
      "localDir": ".",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/appconfig/",
      "localDir": ".",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "destination": "srv-api",
      "csrfProtection": true
    },
    {
      "source": "^/test-srv/(.*)$",
      "target": "/service/risk/$1",
      "destination": "localDestinationTest",
      "csrfProtection": true
    }
  ]
}
